{
    "name": "windwalker/srp",
    "type": "library",
    "license": "MIT",
    "description": "A modern PHP/JS package of SRP-6a (RFC5054). Contains server and client part to help developer use on any cases.",
    "keywords": ["srp", "password", "security", "passwordless"],
    "autoload": {
        "psr-4": {
            "Windwalker\\SRP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Windwalker\\SRP\\Test\\": "test/"
        }
    },
    "authors": [
        {
            "name": "Simon Asika",
            "email": "asika32764@gmail.com"
        }
    ],
    "minimum-stability": "beta",
    "require": {
        "php": ">=8.1",
        "brick/math": "^0.11.0||^0.12.0||^0.13.0"
    },
    "config": {
        "platform": {
            "php": "8.1"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "windwalker/utilities": "^4.0"
    }
}
